{
  "family": "acucare-patient-test",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::326246079459:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "patient-portal",
      "image": "326246079459.dkr.ecr.eu-west-2.amazonaws.com/acucare/patient-portal:test",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {"name": "NODE_ENV", "value": "production"},
        {"name": "NEXT_PUBLIC_API_URL", "value": "https://ALB_DNS/api"},
        {"name": "NEXT_PUBLIC_ENV", "value": "test"}
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/acucare-patient-test",
          "awslogs-region": "eu-west-2",
          "awslogs-stream-prefix": "patient"
        }
      }
    }
  ]
}
