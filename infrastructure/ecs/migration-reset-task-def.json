{
  "family": "acucare-migration-reset",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::326246079459:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "migrations",
      "image": "326246079459.dkr.ecr.eu-west-2.amazonaws.com/acucare/api-backend:migrations",
      "essential": true,
      "command": ["sh", "-c", "python -m alembic downgrade base && python -m alembic upgrade head"],
      "environment": [
        {"name": "ENV", "value": "test"}
      ],
      "secrets": [
        {
          "name": "DATABASE_URL",
          "valueFrom": "arn:aws:secretsmanager:eu-west-2:326246079459:secret:acucare/test/db-credentials-98f8mr:DATABASE_URL::"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/acucare-migrations",
          "awslogs-region": "eu-west-2",
          "awslogs-stream-prefix": "migrations"
        }
      }
    }
  ]
}
